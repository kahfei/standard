<% @need_edit_objs.each do |objs| %>
  <% objs.each do |obj| %>
	  <% if obj.class == AcMotor %>
	    <div id="collapseNeedEditing" class="row">
	      <div class="col-sm-3"><%= link_to "#{obj.arm_part_no}", polymorphic_path(obj, :action => 'correct') %></div>
	      <div class="col-sm-3"><%= obj.release_status %></div>
		  <div class="col-sm-3"><%= obj.class %></div>
		  <div class="col-sm-3"><%= obj.uploader_name %></div>
		</div>
	  <% else %>
	    <div id="collapseNeedEditing" class="row">
	      <div class="col-sm-3"><%= link_to "#{obj.partnumber}", polymorphic_path(obj, :action => 'correct') %></div>
		  <div class="col-sm-3"><%= obj.release_status %></div>
		  <div class="col-sm-3"><%= obj.class %></div>
		  <div class="col-sm-3"><%= obj.uploader_name %></div>
		</div>
	  <% end %>
  <% end %>
<% end %>